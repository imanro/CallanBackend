{
  "name": "Customer",
  "base": "User",
  "idInjection": false,
  "options": {
    "caseSensitiveEmail": false
  },
  "properties": {
    "firstName": {
      "type": "string",
      "required": true
    },
    "lastName": {
      "type": "string",
      "required": false
    },
    "password": {
      "type": "string",
      "required": false
    },
    "description": {
      "type": "string",
      "required": false
    },
    "availableHourInAdvanceMin": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "isActive": {
      "type": "boolean",
      "default": false,
      "nullable": false
    },
    "created": {
      "type": "date",
      "defaultFn": "now"
    },
    "google_api_access_token": {
      "type": "string",
      "required": false
    },
    "google_api_refresh_token": {
      "type": "string",
      "required": false
    },
    "googleApiAccessToken": {
      "type": "string",
      "required": false
    },
    "googleApiRefreshToken": {
      "type": "string",
      "required": false
    }
  },
  "validations": [],
  "relations": {
    "roles": {
      "type": "hasMany",
      "model": "Role",
      "foreignKey": "principalId",
      "through": "RoleMapping",
      "keyThrough": "roleId"
    },
    "Timezone": {
      "type": "belongsTo",
      "model": "TimeZone",
      "foreignKey": "timezoneId"
    }
  },
  "acls": [
