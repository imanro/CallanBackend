{
  "name": "LessonEvent",
  "plural": "LessonEvents",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "startTime": {
      "type": "date",
      "required": true
    },
    "duration": {
      "type": "number",
      "default": 0
    },
    "state": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "cancelationReason": {
      "type": "string"
    },
    "created": {
      "type": "date",
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {
    "CourseProgress": {
      "type": "belongsTo",
      "model": "CourseProgress",
      "foreignKey": "courseProgressId"
    },
    "Lesson": {
      "type": "belongsTo",
      "model": "Lesson",
      "foreignKey": "lessonId"
    },
    "Student": {
      "type": "belongsTo",
      "model": "Customer",
      "foreignKey": "studentId"
    },
    "Teacher": {
      "type": "belongsTo",
      "model": "Customer",
      "foreignKey": "teacherId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "ALLOW",
      "property": "completeLessonEvents"
    }
  ],
  "methods": {}
}
